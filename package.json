{
  "name": "ember-offline-adapter",
  "version": "0.3.5",
  "description": "Store your ember application data in Mozilla's localforage.",
  "keywords": [
    "ember-addon",
    "fastboot",
    "ember-data"
  ],
  "license": "MIT",
  "author": "Paul Chavard",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/tchak/ember-offline-adapter",
  "scripts": {
    "ember": "./node_modules/.bin/ember",
    "build": "yarn ember build",
    "start": "yarn ember server",
    "test": "yarn ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^6.14.0",
    "ember-cli-node-assets": "^0.2.2",
    "localforage": "^1.4.3"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "^3.1.4",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-dependency-checker": "^2.1.1",
    "ember-cli-eslint": "4.2.3",
    "ember-cli-fake-server": "^0.5.0",
    "ember-cli-fastboot": "^1.1.4-beta.1",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-data": "^3.1.1",
    "ember-load-initializers": "^1.1.0",
    "ember-resolver": "^4.5.6",
    "ember-source": "^3.2.0",
    "ember-try": "^0.2.23",
    "loader.js": "^4.0.10"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "ember-addon": {
    "after": "ember-data",
    "configPath": "tests/dummy/config",
    "fastbootDependencies": [
      "crypto"
    ]
  }
}
